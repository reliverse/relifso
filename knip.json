{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": ["**/*.{config,cfg}.{ts,js}", "src/main.ts", "src/**/*-main.ts"],
  "ignore": [
    "reliverse.ts",
    "node_modules",
    "dist-jsr",
