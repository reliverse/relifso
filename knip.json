    "src/impl/lib/copy/index.d.ts",
    "src/impl/lib/copy/index.ts",
    "src/impl/lib/empty/index.d.ts",
    "src/impl/lib/empty/index.ts",
    "src/impl/lib/ensure/file.ts",
    "src/impl/lib/ensure/index.d.ts",
    "src/impl/lib/ensure/index.ts",
    "src/impl/lib/ensure/link.ts",
    "src/impl/lib/ensure/symlink-paths.ts",
    "src/impl/lib/ensure/symlink-type.ts",
    "src/impl/lib/ensure/symlink.ts",
    "src/impl/lib/fs/index.ts",
    "src/impl/lib/json/index.ts",
    "src/impl/lib/json/output-json-sync.ts",
    "src/impl/lib/json/output-json.ts",
