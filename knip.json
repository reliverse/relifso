    "src/impl/lib/output-file/index.ts",
    "src/impl/lib/remove/index.ts",
    "src/impl/lib/util/stat.ts",
    "src/impl/lib/util/utimes.ts"
  ],
  "ignoreBinaries": ["relidler"],
  "ignoreDependencies": [
    "@babel/plugin-transform-block-scoping",
    "@cspotcode/source-map-support",
    "@rollup/plugin-alias",
    "@rollup/plugin-babel",
    "@rollup/plugin-commonjs",
    "@rollup/plugin-node-resolve",
    "@rollup/plugin-replace",
    "@rollup/plugin-terser",
    "@swc/core",
    "@types/eslint",
    "@types/klaw",
    "@types/klaw-sync",
    "@types/minimist",
    "@types/mocha",
    "@types/proxyquire",
    "@types/verror",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "@uwx/fsxt-rollup-plugin-dts",
    "eslint-config-google",
    "klaw",
    "klaw-sync",
    "minimist",
    "mocha",
    "nyc",
    "proxyquire",
    "read-dir-files",
    "rimraf",
    "rollup",
    "rollup-plugin-bundle-size",
    "rollup-plugin-typescript-paths",
    "ts-node",
    "tsconfig-paths",
    "tslib",
    "typedoc",
    "typedoc-plugin-missing-exports",
    "unplugin-swc",
    "verror",
    "@reliverse/cli-cfg"
  ]
